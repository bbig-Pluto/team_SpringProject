<select id="calendarM_List" 
			resultType="CalendarDTO">
				
		 select  * from calendarM 
	
	</select>
	
	<insert id="calendarValueInsert" parameterType="calendarDTO">
	
		insert into calendarM(
			calendarM_time
			, calendarM_Title
			, calendarM_Txt
		)VALUES(
			#{sdfDate}
			,#{calendarM_Title}
			, #{calendarM_Txt}
		)
	
	</insert>

